<%= simple_form_for(@user, :url => { :action => "update_address" }) do |f| %>
  <h2><%= f.label :address, "Adres dostawy" %></h2>
  <%= f.simple_fields_for :address do |a| %>
      <%= a.input :email %>
      <%= a.input :first_name %>
      <%= a.input :last_name %>
      <%= a.input :city %>
      <%= a.input :zip_code %>
      <%= a.input :street %>
    <% end %>
  <%= f.submit %>
<% end %>
